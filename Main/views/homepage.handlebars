<section>
    <h2>BLOG POSTS</h2>
    {{#each blogs as |blog| }}
    <div>
        <div>
            <p>{{blog.title}}</p>
            <p>{{blog.post}}</p>
        </div>
        <div>
            <p>Created by {{user.username}} on {{format_date blog.date_created}}</p>
        </div>
    </div>
    {{/each}}
</section>